{
  "name": "rosbergvik/smp",
  "description": "simple mvc project",
  "keywords": [
    "php",
    "mvc",
    "smp"
  ],
  "version": "0.2",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Victor"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Smp\\": "src/"
    }
  },
  "require": {
    "php": ">=7.3",
    "ext-mbstring": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "vimeo/psalm": "^4.7"
  },
  "autoload-dev": {
    "psr-4": {
      "SmpTest\\": "test/"
    }
  }
}
